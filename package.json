{
  "name": "electron-template",
  "version": "1.0.0",
  "description": "electron-react-redux-webpack-electron",
  "main": "main.js",
  "productName": "Electron-react-webpack",
  "homepage": "./",
  "repository": "https://github.com/rio1228/react-express-mysql-express.git",
  "scripts": {
    "start": "electron .",
    "build": "webpack",
    "build-w": "webpack --watch",
    "component": "asar pack ./out ./project.asar",
    "package-win": "electron-packager ./ electron-template --platform=win32 --arch=x64 --prune=true  --out=release-builds",
    "package-mac": "electron-packager ./ electron-template --platform=win32 --arch=x64 --prune=true  --out=release-builds",
    "package-win-mac": "electron-packager ./ electron-template --platform=win32,darwin --arch=x64 --prune=true  --out=release-builds",
    "repackage-win": "electron-packager . electron-template --platform=win32 --arch=x64 --prune=true  --out=release-builds --overwrite",
    "repackage-mac": "electron-packager . electron-template --platform=darwin --arch=x64 --prune=true  --out=release-builds --overwrite",
    "repackage-win-mac": "electron-packager . electron-template --platform=win32,darwin --arch=x64 --prune=true  --out=release-builds --overwrite",
    "all-package-win": "webpack && asar pack ./out ./project.asar && electron-packager . electron-template --platform=win32 --arch=x64 --prune=true  --out=release-builds",
    "all-package-mac": "webpack && asar pack ./out ./project.asar && electron-packager . electron-template --platform=darwin --arch=x64 --prune=true  --out=release-builds",
    "all-package-win-mac": "webpack && asar pack ./out ./project.asar && electron-packager . electron-template --platform=win32,darwin --arch=x64 --prune=true  --out=release-builds",
    "all-repackage-win": "webpack && asar pack ./out ./project.asar && electron-packager . electron-template --platform=win32 --arch=x64 --prune=true  --out=release-builds --overwrite",
    "all-repackage-mac": "webpack && asar pack ./out ./project.asar && electron-packager . electron-template --platform=darwin --arch=x64 --prune=true  --out=release-builds --overwrite",
    "all-repackage-win-mac": "webpack && asar pack ./out ./project.asar && electron-packager . electron-template --platform=win32,darwin --arch=x64 --prune=true  --out=release-builds --overwrite"
  },
  "keywords": [],
  "author": "Ryo Mikami",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.17.2",
    "electron": "^1.7.5",
    "express": "^4.15.4",
    "express-generator": "^4.15.0",
    "mysql": "^2.14.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "superagent": "^3.5.2",
    "webpack": "^3.5.2"
  },
  "devDependencies": {
    "webpack-dev-server": "^2.7.1"
  }
}
